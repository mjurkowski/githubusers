{
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
      "extensions": [".html", ".js", ".tsx", ".ts"]
  },
  "eslint.autoFixOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "typescript.tsdk": "./node_modules/typescript/lib"
}
